# Shodan API Configuration
SHODAN_API_KEY=your_shodan_api_key_here

# Output Configuration
OUTPUT_DIR=./output
LOG_LEVEL=INFO

# Scan Configuration
MAX_RESULTS_PER_QUERY=1000
ENABLE_HOST_ENRICHMENT=true
MAX_ENRICHMENTS=100

# Rate Limiting
RATE_LIMIT_DELAY=1
QUERY_COOLDOWN=2

# Report Configuration
GENERATE_CSV=true
GENERATE_JSON=true
GENERATE_CHARTS=true
